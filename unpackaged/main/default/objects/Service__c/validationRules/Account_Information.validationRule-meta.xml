<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Information</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( Status__c , &#39;Done&#39;)
,

 KWA_Account_information__c= FALSE )</errorConditionFormula>
    <errorDisplayField>KWA_Account_information__c</errorDisplayField>
    <errorMessage>KWA cannot be completed without the account information.</errorMessage>
</ValidationRule>
