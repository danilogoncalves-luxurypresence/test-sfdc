<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Subscription_Extension_Date</fullName>
    <active>false</active>
    <errorConditionFormula>AND (
Subscription_extension_date__c &gt;  Subscription_start_date__c, 
Subscription_start_date__c &lt;= TODAY()
)</errorConditionFormula>
    <errorDisplayField>Subscription_extension_date__c</errorDisplayField>
    <errorMessage>The start date has begun to make any changes please contact billing.</errorMessage>
</ValidationRule>
