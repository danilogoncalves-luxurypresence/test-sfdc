<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ad_Spend_Difference__c</fullName>
    <formula>IF( Record_Type_Name__c = &quot;Reduce Service&quot;,
(Ad_Spend__c *- 1), Ad_Spend__c  -  Current_Ad_Spend__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Ad Spend Difference</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
