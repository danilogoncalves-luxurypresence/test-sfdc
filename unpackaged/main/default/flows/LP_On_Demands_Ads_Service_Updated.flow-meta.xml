    <formulas>
        <name>for1BDay</name>
        <dataType>Date</dataType>
        <expression>CASE( 
  MOD({!$Flow.CurrentDate} - DATE( 1900, 1, 7 ), 7 ),
  5, {!$Flow.CurrentDate} + 3,
  6, {!$Flow.CurrentDate} + 2,
  {!$Flow.CurrentDate} + 1
)</expression>
    </formulas>
    <formulas>
        <name>LastDay</name>
        <dataType>Date</dataType>
        <expression>IF( MONTH( {!DateValue} ) = 12, 
DATE( YEAR( {!DateValue} ), 12, 31 ), 
DATE( YEAR( {!DateValue} ), MONTH({!DateValue} ) + 1, 1) - 1)</expression>
    </formulas>
    <formulas>
        <name>Month</name>
        <dataType>String</dataType>
